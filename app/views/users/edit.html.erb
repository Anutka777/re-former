<h1>Edit profile</h1>

<%= form_with model: @user do |f| %>
  <%= f.label :username, 'Username' %>
  <%= f.text_field :username%>
  <br>
  <br>
  <%= f.label :email, 'E-mail' %>
  <%= f.email_field :email %>
  <br>
  <br>
  <%= f.label :password, 'Password' %>
  <%= f.password_field :password %>
  <br>
  <br>
  <%= f.submit %>
<% end %>